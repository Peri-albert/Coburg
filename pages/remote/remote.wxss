view {
  width: 600rpx;
  height: 645rpx;
  position: absolute;
  margin-top: 200rpx;
  margin-left: 75rpx;
}

image {
  width: 200rpx;
  height: 200rpx;
  float: left;
}

navigator, .middle {
  width: 200rpx;
  position: absolute;
}

.middle {
  margin-left: 200rpx;
}

view navigator:nth-child(2) {
  margin-left: 400rpx;
}

view navigator:nth-child(3) {
  margin-top: 200rpx;
  margin-left: 200rpx;
}

view navigator:nth-child(4) {
  margin-top: 400rpx;
}

view navigator:nth-child(5) {
  margin-top: 400rpx;
  margin-left: 400rpx;
}

text {
  float: left;
  width: 200rpx;
  text-align: center;
  color: #d2d2d2;
}